{
  "name": "hubot-ringcentral-sms",
  "version": "0.2.0",
  "description": "RingCentral SMS Adapter for Hubot",
  "scripts": {
	"prepublish": "coffee --join hubot-ringcentral-sms.js --compile src/*.coffee",
	"preversion": "npm test",
	"version": "npm run build && git add -A dist",
	"postversion": "git push && git push --tags && rm -rf build/temp"
	"test": "echo \"Error: no test specified\" && exit 1"
  },
  "main": "./src/ringcentral",
  "engines": {
	"node": ">=4.6.x",
	"npm": ">=3.10.x"
  },
  "dependencies": {
    "parent-require": "^1.0.0",
    "ringcentral": "^3.0.0-rc1"
  },
  "peerDependencies": {
    "hubot": ">=2.0"
  },
	"devDependencies": {
	  "coffee-script": ">=1.2.0"
	},
  "repository": {
    "type": "git",
    "url": "https://github.com/LingForCC/hubot-ringcentral-sms.git"
  },
